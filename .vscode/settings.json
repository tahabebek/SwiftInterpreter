{
  "[swift]": {
    "editor.defaultFormatter": "sweetpad.sweetpad",
    "editor.formatOnSave": true,
  },
  "sweetpad.format.path": "swift-format",
  "sweetpad.format.args": ["--configuration", "swift-format.json"],
  "sweetpad.system.taskExecutor": "v2",
  "task.autoDetect": "on",
  "sweetpad.build.arch": "arm64",
  "sweetpad.system.logLevel": "debug",
  "taskExplorer.exclude": [
    "**/.vscode-test/**",
    "**/Assets/**",
    "**/bin/**",
    "**/build/**",
    "**/dist/**",
    "**/doc/**",
    "**/example/**",
    "**/examples/**",
    "**/ext/**",
    "**/obj/**",
    "**/out/**",
    "**/output/**",
    "**/packages/**",
    "**/release/**",
    "**/releases/**",
    "**/samples/**",
    "**/sdks/**",
    "**/static/**",
    "**/target/**",
    "**/test/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/work/**",
    "/Users/tahabebek/Projects/Interpreter/.build/checkouts/swift-argument-parser/Scripts/environment.sh",
    "/Users/tahabebek/Projects/Interpreter/.derivedData/SourcePackages/checkouts/swift-argument-parser/Scripts/environment.sh"
  ],
  "sweetpad.system.enableSentry": true,
  "taskExplorer.excludeTask": [
    "pre-launch[0-9]{2}\\.r(?:b|c)",
    "echo"
  ],
  "swift.path": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
  "swift.sourcekit-lsp.trace.server": "messages",
  "swift.swiftEnvironmentVariables": {
    "DEVELOPER_DIR": "/Applications/Xcode.app"
  }
}
